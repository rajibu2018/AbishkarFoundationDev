        <script type="text/javascript">
            $(document).ready(function () {
                $("#submit").click(function () {
                    var Name = $("#name").val();
                    var Email = $("#email").val();
                    var Description = $("#msg").val();

                    if (Name == "") {
                        alert("Please enter your Name.");
                        $("#name").focus();
                        return (false);
                    }

                    if (Email == "") {
                        alert("Please enter your Email.");
                        $("#email").focus();
                        return (false);
                    }
                    else {

                    }
                    if (Description == "") {
                        alert("Please enter your Message.");
                        $("#Message").focus();
                        return (false);
                    }



                    var dataString = 'Name=' + Name + '&Email=' + Email + '&Description=' + Description;
                    dataString = dataString.replace(/ /gi, "%20");
                    //alert (dataString);return false;
                    $.ajax({
                        type: "POST",
                        url: "ContactProcess.php",
                        data: dataString,
                        success: function (result) {
                            // alert(result);
                            if (result.trim() == "InvalidCode") {
                                alert("Please enter valid verification code.");
                                $("input#FSecurityCode").focus();
                                return (false);
                            }
                            else {
                                $('#Enquiry').html("<div id='Enquiry' style='text-align:left;padding:10px;'></div>");
                                $('#Enquiry').html("<span style='font-weight:bold;'><br>Thank you! Your information has been successfully submitted and you will receive a response from someone at our office shortly.")
                                    .hide()
                                    .fadeIn(1500);
                                //.delay(4000).fadeOut(1500);
                                $("#name").val('');
                                $("#email").val('');
                                $("#msg").val('');
                            }
                        }
                    });
                    return false;
                });
            });
        </script>
